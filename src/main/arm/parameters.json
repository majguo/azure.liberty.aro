{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "${artifactsLocationBase}/src/main/"
        },
        "createCluster": {
            "value": "${createCluster}"
        },
        "clusterName": {
            "value": "${clusterName}"
        }, 
        "clusterRGName": {
            "value": "${clusterRGName}"
        },
        "deployWLO": {
            "value": "${deployWLO}"
        },
        "edition": {
            "value": "${edition}"
        },
        "productEntitlementSource": {
            "value": "${productEntitlementSource}"
        },
        "deployApplication": {
            "value": "${deployApplication}"
        },
        "appImagePath": {
            "value": "${appImagePath}"
        },
        "autoScaling": {
            "value": "${autoScalingParam}"
        },
        "appReplicas": {
            "value": "${appReplicas}"
        }
    }
}